
@{
    ViewBag.Title = "loginAudit";
    var dt=ViewBag.Data;
}
<h2>loginAudit</h2>


<table class="table table-bordered display">
    <thead>
        <tr>
            @foreach (var h in dt.Columns)
            {
                <th>@h</th>
            }
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < dt.Rows.Count; i++)
        {
            <tr>
                @foreach (var h in dt.Columns)
                {
                    <td>@dt.Rows[i][h]</td>
                }
            </tr>
        }
    </tbody>
</table>
@Html.Partial("_loading")
<script>
    $(document).ready(() =>{
       
        $("table").DataTable({
            dom: 'QBlfrtip',
            order: [
                     [0, 'desc']
                   ]
        });
    })
</script>